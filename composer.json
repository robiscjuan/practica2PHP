{
    "name": "miw_upm/miw16_results",
    "license": "MIT",
    "type": "project",
    "description": "MiW16 Results",
    "keywords": ["microframework", "rest", "slim", "doctrine", "psr7"],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "http://miw.etsisi.upm.es/",
            "email": "miw@etsisi.upm.es"
        }
        ],
    "autoload": {
        "psr-4": {
            "MiW16\\Results\\Entity\\": "src/Entity/",
            "MiW16\\Results\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiW16\\Results\\Tests\\": "tests/"
        }
    },
    "require": {
		"php": ">=5.5.0",
        "doctrine/orm": "^2.5",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "zircote/swagger-php": "^2.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8000 -t public",
        "test": "phpunit"
    }
}